<div class="form-container">

  <h1 class="form-header">Add a Bookmark <%= @list.name %></h1>

  <%= simple_form_for([@list, @bookmark]) do |f| %>
    <%= f.input :comment, input_html: { class: 'form-control' } %>
    <%= f.input :movie_id, input_html: { class: 'form-control' } %>
    <%= f.submit class: 'submit-button' %>
  <% end %>
</div>
